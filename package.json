{
  "name": "@didomi/react-native",
  "version": "2.21.0",
  "description": "Didomi React Native SDK",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-didomi.podspec",
    "!lib/typescript/sample",
    "!lib/typescript/test",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "app.plugin.js",
    "plugin/"
  ],
  "workspaces": [
    ".",
    "sample",
    "test"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "prepare": "bob build",
    "pods": "cd test && pod-install --quiet"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/didomi/react-native.git"
  },
  "author": "Didomi",
  "license": "BSD-3",
  "homepage": "https://www.didomi.io/",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@rnx-kit/metro-config": "^2.1.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^19.1.0",
    "jest": "^29.7.0",
    "pod-install": "^0.2.2",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-builder-bob": "^0.40.0",
    "react-native-monorepo-tools": "^1.2.1",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": ">=0.77.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/sample/node_modules",
      "<rootDir>/test/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@expo/config-plugins": "^54.0.2"
  },
  "expo": {
    "plugins": [
      "./plugin/withDidomi.js"
    ]
  }
}
